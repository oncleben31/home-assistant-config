---
# wether component using data from meteo France
#
# https://www.home-assistant.io/components/meteo_france/
#
meteo_france_custom:
  - city: !secret city
    monitored_conditions:
      - rain_chance
      - freeze_chance
      - thunder_chance
      - snow_chance
      - weather
      - next_rain
      - wind_speed
      - temperature
      - uv
      - weather_alert
